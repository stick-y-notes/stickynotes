{
  "name": "stickynotes",
  "private": true,
  "version": "0.0.0",
  "source": "src/index.html",
  "scripts": {
    "start": "parcel",
    "dev": "live-server src --port=7027",
    "build": "rm -rf dist && parcel build",
    "clean": "rm -rf dist && rm -rf .parcel-cache",
    "web4_testnet": "web4-deploy src stickynotes.testnet --nearfs --yes",
    "web4_mainnet": "web4-deploy src stickynotes.near --nearfs"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.14.4",
    "@parcel/transformer-webmanifest": "2.14.4",
    "parcel": "^2.14.0",
    "svgo": "^3",
    "live-server": "^1.2.2"
  },
  "packageManager": "pnpm@10.8.1"
}
